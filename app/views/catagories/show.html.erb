<div class="row-fluid">
	<div class="span10">
		<div class="row-fluid">
		
		<%= link_to image_tag("kidstack65x65alternate4.png", :alt =>"kidstack logo", :class => "topnav"), root_path %>

		<div class="span1">
		<%= image_tag("arrow.png", :alt => "arrow pointing right", :class => "topnav") %>
		</div>
		<div class="span2">
		<%= link_to image_tag(@catagory.navgraphic, :class => "topnav"), catagory_path(@catagory.name) %>
		</div>


		</div>
		<div class="row-fluid">	
			<div class = "span12">
			</div>
		</div>
		
		
		<div class="row-fluid">			
			<div id="show_slide">
				<%=  render( :partial => "catslide", :locals => { :slide => @slide} ) %>
			</div>
		</div>
	</div>
	
		
	<div class="span2">
		<div class="row-fluid">
			<div class="span2">

			</div>
			<div class="span10">
				<% @catagory.topics.each do |topic| %>
					<%= link_to image_tag(topic.navgraphic, :class => "navgraph"), topic_path(topic.name) %>
				<% end %>
			</div>
			
		</div>
	</div>
			
</div>





