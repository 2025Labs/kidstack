<div class="row-fluid">
	<div class="span9">
		<div class="row-fluid">
		<%= link_to image_tag("kidstack65x65alternate4.png", :alt =>"kidstack logo", :class => "topnav"), root_path %>

		<%= link_to image_tag("navgraphics/structurestext.png", :alt =>"structures", :class => "cattext"), catagory_path(@catagory.name) %>
		</div>

		
		<div class="row-fluid">			
			<div id="show_slide">
				<%=  render( :partial => "catslide", :locals => { :slide => @slide} ) %>
			</div>
		</div>
	</div>
	
		
	<div class="span3">
		<div class="row-fluid">
			<div class="span3">
			</div>
			
			<div class="span9">
				<% @catagory.topics.each do |topic| %>
					<%= link_to image_tag(topic.navgraphic, :class => "navgraph"), topic_path(topic.name) %>
				<% end %>
			</div>
			
		</div>
	</div>
			
</div>





