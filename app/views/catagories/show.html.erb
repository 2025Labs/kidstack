<div class="row-fluid">
	<div class="span9">
		<div class="row-fluid">
		<%= link_to image_tag("kidstack65x65alternate4.png", :alt =>"kidstack logo", :class => "topnav"), root_path %>
		<h1>
			<div class = "cattext">
				<a class="<%= @catagory.color %>" <%= link_to @catagory.headergraphic, catagory_path(@catagory.name) %></a>
			</div>
		  </h1>
		</div>
		<div class="row-fluid">
			<div class="span12">
			</div>
		</div>
		
		
		<div class="row-fluid">			
			<div id="show_slide">
				<%=  render( :partial => "catslide", :locals => { :slide => @slide} ) %>
			</div>
		</div>
		
		<div class="row-fluid">	
			<hr>
			<h3>
			<div class = "cattext">
				<span class="<%= @catagory.color %>"> timeline</span>
			</div>
			</h3>
			<%= image_tag("structurestimelineforks.png")%>
		</div>
		
		
	</div>
	
		
	<div class="span3">
		<div class="row-fluid">
			<div class="span2">
			</div>
			
			<div class="span2">
					<%= image_tag("pointingfigure.png") %>
			</div>
			
			<div class="span1">
			</div>
			
			<div class="span7">
				<% @catagory.topics.each do |topic| %>
					<%= link_to image_tag(topic.navgraphic, :class => "navgraph"), topic_path(topic.name) %>
				<% end %>
			</div>
			
		</div>
	</div>
			
</div>





